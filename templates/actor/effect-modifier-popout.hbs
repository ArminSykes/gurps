<div class='gga-popup-window'>
  <ul class='modifier-list'>
    {{#each selfmodifiers}}
      <li>{{{this}}}</li>
    {{/each}}
  </ul>
  {{#if targetmodifiers}}
    <div class='heading'>{{i18n 'GURPS.targetModifiers' 'Target Modifiers'}}</div>
    <ul class='modifier-list'>
      {{#each targetmodifiers}}
        <li>{{{this}}}</li>
      {{/each}}
    </ul>
  {{/if}}
</div>